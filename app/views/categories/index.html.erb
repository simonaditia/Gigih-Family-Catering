<h1>categories#index</h1>
<p>Find me in app/views/categories/index.html.erb</p>
<table>
  <thead>
    <tr>
        <td><b>Name</b></td>
    </tr>
  </thead>
  <tbody>
    <% @categories.each do |category| %>
      <tr>
        <td><%= category.name %></td>
        <td><%= link_to 'show', {controller: 'categories', action: 'show', id: category.id} %></td>
        <td><%= link_to 'edit', {controller: 'categories', action: 'edit', id: category.id} %></td>
        <td><%= button_to 'delete', category_path(category.id), :method => :delete %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<a href="/categories/new">New category</a>