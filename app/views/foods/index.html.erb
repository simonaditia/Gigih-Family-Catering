<h1>Foods#index</h1>
<p>Find me in app/views/foods/index.html.erb</p>
<table>
  <thead>
    <tr>
        <td><b>Name</b></td>
        <td><b>Price</b></td>
        <td><b>Description</b></td>
    </tr>
  </thead>
  <tbody>
    <% @foods.each do |food| %>
      <tr>
        <td><%= food.name %></td>
        <td><%= food.price %></td>
        <td><%= food.description %></td>
        <td><%= link_to 'show', {controller: 'foods', action: 'show', id: food.id} %></td>
        <td><%= link_to 'edit', {controller: 'foods', action: 'edit', id: food.id} %></td>
        <td><%= button_to 'delete', food_path(food.id), :method => :delete %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<a href="/foods/new">New food</a>